<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.xujianhuaap.xwheelview" >

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme" >
        <activity android:name=".MainActivity" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".FirstActivity" android:label="First"></activity>
        <activity android:name=".SecondActivity" android:launchMode="standard" android:label="Second"></activity>
        <activity android:name=".ThirdActivity" android:launchMode="standard" android:label="Third"></activity>
    </application>

</manifest>